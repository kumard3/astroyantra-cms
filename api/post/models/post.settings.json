{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "posts",
      "dominant": true
    },
    "likes": {
      "type": "integer",
      "default": 0
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
